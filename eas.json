{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "scheme": "App",
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "scheme": "App",
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "ios": {
        "scheme": "App",
        "buildConfiguration": "Release"
      }
    }
  },
  "builds": {
    "ios": {
      "projectRootDirectory": "ios/App"
    },
    "android": {
      "projectRootDirectory": "android"
    }
  },
  "submit": {
    "production": {}
  }
}
